@{
	ViewBag.Title = "Inicio";

	ViewBag.canMate = "100";
	ViewBag.alumnosAde = "5";

	List<string> alumnos = new List<string>();
	alumnos.Add("Angel");
	alumnos.Add("Leo");
	alumnos.Add("Omar");
	alumnos.Add("Cholo");
	alumnos.Add("Toño");
}

	<div class="row container area">
		<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
			<h2>Panel de control</h2>
		</div>
		<div class="area-cards">
			<div class="card cgreen">
				<h4>Stock total</h4>
				<h2>@ViewBag.canMate materiales</h2>
				<p><a class="btn btn-default" href="https://go.microsoft.com/fwlink/?LinkId=301865">Ver más &raquo;</a></p>
			</div>
			<div class="card corange">
				<h4>Alumnos con adeudo</h4>
				<h2>@ViewBag.alumnosAde Alumnos con adeudo</h2>
				<p><a class="btn btn-default" href="https://go.microsoft.com/fwlink/?LinkId=301866">Ver más &raquo;</a></p>
			</div>
			<div class="card cred">
				<h4>Material dado de baja</h4>
				<h2>6 materiales</h2>
				<p><a class="btn btn-default" href="https://go.microsoft.com/fwlink/?LinkId=301867">Ver más &raquo;</a></p>
			</div>
		</div>
		<div class="table container table-responsive-md">
			<table class="table table-hover dtable">
				<h3>Alumnos con prestamos atrasados</h3>
				<thead>
					<tr>
						<th scope="col">#</th>
						<th scope="col">Nombre</th>
						<th scope="col">Carrera</th>
						<th scope="col">Grupo</th>
						<th scope="col">Fecha prestamo</th>
						<th>Acciones</th>
					</tr>
				</thead>
				<tbody>
					@foreach (var x in alumnos)
					{
						<tr>
							<th scope="row">1</th>
							<td>@x.ToString()</td>
							<td>Ott1</td>
							<td>ITI 806</td>
							<td>05/04/2020</td>
							<td class="acciones">
								<ul>
									<li>@Html.ActionLink("Editar", "Contact", "Home", null, new { @class = "editar" })</li>
									<li>@Html.ActionLink("Eliminar", "Contact", "Home", null, new { @class = "eliminar" })</li>
								</ul>
							</td>
						</tr>
					}
				</tbody>
			</table>
		</div>
	</div>